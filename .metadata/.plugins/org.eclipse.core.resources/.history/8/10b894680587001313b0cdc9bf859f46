import java.util.HashMap;


public class SitesHashMap extends HashMap<String, Integer> {
	
	public void insertSite(String s)
	{
		if(this.containsKey(s))
		{
			this.put(s, this.get(s)+1);
		}
		else
		{
			this.put(s, 1);
		}
	}
	
	public MaxHeap createMaxHeap()
	{
		MaxHeap heap = new MaxHeap(this.size());
		Set<Entry<String,Integer>> entries = this.entrySet();
		
	}

}
